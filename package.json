    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/schedule": "^4.1.1",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "https-proxy-agent": "^7.0.5",
    "ioredis": "^5.4.1",
    "mongoose": "^8.7.0",
    "nanoid": "^5.0.7",
    "p-limit": "^3.1.0",
    "puppeteer": "^23.4.0",
    "puppeteer-extra": "^3.3.6",
